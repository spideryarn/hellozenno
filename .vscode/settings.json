{
  "[typescript]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "deno.enablePaths": [
    "supabase/functions"
  ],
  "deno.lint": true,
  "deno.unstable": [
    "bare-node-builtins",
    "byonm",
    "sloppy-imports",
    "unsafe-proto",
    "webgpu",
    "broadcast-channel",
    "worker-options",
    "cron",
    "kv",
    "ffi",
    "fs",
    "http",
    "net"
  ],
  "python.testing.pytestArgs": [
    "."
  ],
  "python.testing.pytestEnabled": true,
  "python.testing.unittestEnabled": false,
  "terminal.integrated.env.osx": {
    "GOOGLE_APPLICATION_CREDENTIALS": "${workspaceFolder}/credentials/google-service-account-key.json"
  }
}
